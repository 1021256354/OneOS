OneOS.LoadAPI('/System/API/Bedrock.lua', false)

local program = Bedrock:Initialise()

program:RegisterKeyboardShortcut({'\\'}, function()os.reboot()end)

local function setQuote()
	local h = fs.open('/Resources/FuckYou.data', 'r')
	if h then
		local quotes = textutils.unserialize(h.readAll())
		h.close()
		program:GetObject('FuckYouLabel').Text = quotes[math.random(1, #quotes)]
	end
end

program:Run(function()
	setQuote()
end)