	function Start()
		Turtle.SetHome()

		Turtle.SetState('Excavating...')
		for d = 1, 2 do--Current.Excavator.depth do
			Turtle.DigMoveDown(1)
			for w = 1, 3 do
				Turtle.DigMoveForward(3)
				print('-------')
				print(w)
				print(w % 2)
				if (w % 2) == 1 then
					print('right')
					Turtle.TurnRight()
					Turtle.DigMoveForward(1)
					Turtle.TurnRight()
				else
					print('left')
					Turtle.TurnLeft()
					Turtle.DigMoveForward(1)
					Turtle.TurnLeft()
				end
			end
			Turtle.GoHome(nil, false, nil)--nil, nil, false)
		end

		--Turtle.GoHome()
	end